{
  "spring": {
    "application": {
      "name": "DB-SERVICE"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "port": "7224",
      "ipAddress": "localhost"
      //"preferIpAddress": true
    },
    "Logging": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Warning",
        "Grpc": "Debug"
      }
    },
    "AllowedHosts": "*"
  }
}